{
  "name": "@qdocs/monorepo",
  "packageManager": "yarn@4.3.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "next": "yarn workspace @qdocs/next dev",
    "next:build": "yarn workspace @qdocs/next build",
    "start": "yarn workspace @qdocs/app start",
    "build": "yarn workspace @qdocs/app build"
  }
}
